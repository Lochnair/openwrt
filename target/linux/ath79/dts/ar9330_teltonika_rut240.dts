/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9330.dtsi"

/ {
	compatible = "teltonika,rut240", "qca,ar9330";
	model = "Teltonika RUT240";

	chosen {
		bootargs = "console=ttyATH0,115200 root=31:05 rootfstype=squashfs";
	};


	aliases {
		serial0 = &uart;
		led-boot = &signal3;
		led-failsafe = &signal3;
		led-upgrade = &signal3;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 22 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};

		input {
			label = "input";
			linux,code = <BTN_0>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		sim {
			label = "SIM holder";
			linux,code = <KEY_RFKILL>;
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		signal1 {
			label = "rut240:green:signal1";
			gpios = <&gpio 23 GPIO_ACTIVE_LOW>;
		};

		signal2 {
			label = "rut240:green:signal2";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
		};

		signal3: signal3 {
			label = "rut240:green:signal3";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
		};

		signal4 {
			label = "rut240:green:signal4";
			gpios = <&gpio 26 GPIO_ACTIVE_LOW>;
		};

		signal5 {
			label = "rut240:green:signal5";
			gpios = <&gpio 27 GPIO_ACTIVE_LOW>;
		};

		status_2g {
			label = "rut240:green:status_2g";
			gpios = <&gpio 8 GPIO_ACTIVE_HIGH>;
		};

		status_3g {
			label = "rut240:green:status_3g";
			gpios = <&gpio 24 GPIO_ACTIVE_HIGH>;
		};

		status_4g {
			label = "rut240:green:status_4g";
			gpios = <&gpio 21 GPIO_ACTIVE_HIGH>;
		};

		lan {
			label = "rut240:green:lan";
			gpios = <&gpio 14 GPIO_ACTIVE_HIGH>;
		};

		wan {
			label = "rut240:green:wan";
			gpios = <&gpio 17 GPIO_ACTIVE_HIGH>;
		};
	};
};

&spi {
	status = "okay";
	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				reg = <0x0 0x20000>;
				label = "u-boot";
				read-only;
			};

			config: partition@20000 {
				reg = <0x20000 0x10000>;
				label = "config";
				read-only;
			};

			art: partition@30000 {
				reg = <0x30000 0x10000>;
				label = "art";
				read-only;
			};

			firmware: partition@40000 {
				compatible = "tplink,firmware";
				reg = <0x40000 0xf30000>;
				label = "firmware";
			};

			event_log: partition@f70000 {
				reg = <0xf70000 0x90000>;
				label = "event-log";
				read-only;
			};
		};
	};
};

&eth0 {
	status = "okay";

	mtd-mac-address = <&config 0x0>;

	gmac-config {
		device = <&gmac>;

		switch-phy-addr-swap = <0>;
		switch-phy-swap = <0>;
	};
};

&eth1 {
	status = "okay";

	mtd-mac-address = <&config 0x0>;
	mtd-mac-address-increment = <1>;
};

&gpio {
	status = "okay";

	gpio_usb_power {
		gpio-hog;
		gpios = <19 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "rut240:usb:power";
	};
};

&uart {
	status = "okay";
};

&usb {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&config 0x0>;
	mtd-mac-address-increment = <2>;
};
